{
  "name": "git-account",
  "version": "1.0.0",
  "description": "git-account adds user management feature to `git`.",
  "main": "dist",
  "scripts": {
    "build": "tsc",
    "test": "npm run build && nyc ava --verbose",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "watch": "tsc -w"
  },
  "bin": {
    "git-account": "bin/git-account"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/uetchy/git-account.git"
  },
  "keywords": [
    "git",
    "management",
    "account"
  ],
  "author": "Yasuaki Uechi <y@uechi.io> (https://uechi.io/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/uetchy/git-account/issues"
  },
  "homepage": "https://github.com/uetchy/git-account#readme",
  "dependencies": {
    "chalk": "^2.4.1",
    "columnify": "^1.5.4",
    "commander": "^2.15.1",
    "execa": "^1.0.0",
    "file-exists": "^5.0.1",
    "ini": "^1.3.5",
    "inquirer": "^6.2.0",
    "update-notifier": "^2.5.0"
  },
  "devDependencies": {
    "@types/execa": "^0.9.0",
    "@types/file-exists": "^4.0.0",
    "@types/ini": "^1.3.29",
    "ava": "^0.25.0",
    "coveralls": "^3.0.1",
    "mockery": "^2.1.0",
    "nyc": "^13.0.1",
    "typescript": "^3.0.1"
  }
}
